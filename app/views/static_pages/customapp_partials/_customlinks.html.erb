<div class="row">
<% if logged_in? %>
    <div class="col-md-4 col-md-offset-2">
      <%= link_to "comparte en redes sociales!", '', id:'guardar-nueva-customapp', class:'btn btn-info'%>
    </div>
    <div class="col-md-4 ">
      <%= link_to "cotizar!", '', id:'guardar-nueva-customapp-cotiza', class:'btn btn-success'%>
    </div>
<% else %>
  <% store_location %>
    <div class="col-md-6 col-md-offset-3">
    <%= link_to "registrate para guardar y compartir tu moto", save_changes_path, method: 'post', class:'btn btn-primary'%>
  </div>
  <% end %>
</div>


